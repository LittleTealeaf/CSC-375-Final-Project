{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Statement1",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:*",
      "Resource": "arn:aws:s3:::sample-bucket-dn/*",
      "Condition": {
        "IpAddress": {
          "aws:SourceIp": "68.230.140.43/32"
        }
      }
    },
    {
      "Sid": "Statement2",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:*",
      "Resource": "arn:aws:s3:::sample-bucket-dn/*",
      "Condition": {
        "ForAnyValue:StringEquals": {
          "aws:Referer": "1234"
        }
      }
    }
  ]
}
